<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.colettak.breakin" android:versionName="1.0" android:versionCode="1" xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>

    <application android:label="@string/app_name" android:icon="@drawable/icon">
        <activity android:name="com.colettak.breakin.SplashExample"
                  android:label="@string/app_name"
                  android:debuggable="true">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
        
        <activity android:name="com.colettak.breakin.Main"
                  android:label="@string/app_name">
        </activity>

    </application>
</manifest>